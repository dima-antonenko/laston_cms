<div id="page_content_inner">
    <div class="uk-grid uk-grid-medium" data-uk-grid-margin>
        <div class="uk-width-xLarge-8-10  uk-width-large-10-10">
            <div class="md-card">
                <div class="md-card-content">
                    <div class="uk-overflow-container uk-margin-bottom">
                        <table class="uk-table uk-table-align-vertical uk-table-nowrap tablesorter tablesorter-altair" id="ts_pager_filter">
                            <thead>
                                <tr>
                                    <th>id</th>
                                    <th>имя</th>
                                    <th>телефон</th>
                                    <th>дата создания</th>
                                    <th></th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                <% @form_requests.each do |item| %>
                                <tr>
                                    <td><%= item.id %></td>
                                     <td><%= item.name %></td>
                                     <td><%= item.phone %></td>
                                     <td><%= item.created_at %></td>
                                    <td><%= link_to 'просмотр', administrators_form_request_path(item) %></td>
                                    <td><%= link_to 'удалить', administrators_form_request_path(item), method: :delete %></td>

                                </tr>
                                <% end %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            
        </div>
    </div>
    
</div>